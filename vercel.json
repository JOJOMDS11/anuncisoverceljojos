{
  "version": 2,
  "name": "discord-announcement-panel",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_BASE_URL": "https://discord-anuncios.onrender.com/api"
  }
}
